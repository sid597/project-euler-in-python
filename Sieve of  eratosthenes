def primes_up_to(x):

    """

    Returns a list of primes less than equal to x
    using Sieve of Eratosthenes : https://en.wikipedia.org/wiki/Sieve_of_Eratosthenes

    >>> primes_list= primes_up to(20)
    >>>print primes_list
    [2, 3, 5, 7, 11, 13, 17, 19]

    :param x: The number up to which primes are to be generated
    :return: A list of primes up to ``x``

"""

    l = [True]*x                       # A boolean list to store all probable prime
    for num in xrange(2, int(x**0.5 + 1.5)):   # For numbers in range 2 to sqrt(x)
        if l[num]:                         # If num is prime
            yield num
            for j in xrange(num, x, num):
                l[j] = False           # Mark all of its multiples False
    for i in xrange(2, x):
        if l[i]:                 # If prime then yield it
            yield i
k = list(primes_up_to(20))

print k


